{
  "scripts": {
    "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
    "replica_start": "dfx start --background",
    "replica_stop": "dfx stop",
    "canister_deploy_local": "dfx deploy azle_by_example",
    "canister_deploy_mainnet": "dfx deploy --network ic azle_by_example",
    "canister_uninstall": "dfx canister uninstall-code azle_by_example",
    "canister_call_get_message": "dfx canister call azle_by_example getMessage",
    "canister_call_set_message": "dfx canister call azle_by_example setMessage '(\"Hello world!\")'"
  },
  "dependencies": {
    "azle": "0.19.0"
  }
}
